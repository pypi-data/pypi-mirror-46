# Other unique rule patterns/combinations not specified elsewhere

{
  # left bank
  "ent.":   ["SPW",        "ent",         [], "prefix `ent` (SPWER → enter)"],
  "int.":   ["SPW",        "int",         [], "prefix `int` (SPWELGT → intelligent)"],
  "enter.": ["SPWR",       "(ent.)e(r.)", [], "ONLY works as prefix"],
  "inter.": ["SPWR",       "(int.)e(r.)", [], "ONLY works as prefix"],

  # right bank
  ".ded":   ["-TD",        "[d|.t](.ed)", [], "use -T for first of two `d`s (standard?) (SKWRAEUTD → jaded)"],
  ".xed":   ["-BGD",       "[x|.k](.ed)", [], "treat `x` as `k` to avoid -SD combo (PLEUBGD → mixed)"],

  # split/slurred strokes
  "b/b":    ["-B/PW",      "b",     ["RARE"], "consonant `b` split between strokes"],
  "c/c":    ["-BG/K",      "c",     ["RARE"], "hard `c` split between strokes"],
  "C/C":    ["-S/S",       "c",     ["RARE"], "soft `c` split between strokes"],
  "d/d":    ["-D/TK",      "d",     ["RARE"], "consonant `d` split between strokes"],
  "f/f":    ["-F/TP",      "f",     ["RARE"], "consonant `f` split between strokes"],
  "g/g":    ["-G/TKPW",    "g",     ["RARE"], "hard `g` split between strokes"],
  "G/G":    ["-PBLG/SKWR", "g",     ["RARE"], "soft `g` split between strokes"],
  "j/j":    ["-PBLG/SKWR", "j",     ["RARE"], "consonant `j` split between strokes"],
  "k/k":    ["-BG/K",      "k",     ["RARE"], "consonant `k` split between strokes"],
  "l/l":    ["-L/HR",      "l",     ["RARE"], "consonant `l` split between strokes"],
  "m/m":    ["-PL/PH",     "m",     ["RARE"], "consonant `m` split between strokes"],
  "n/n":    ["-PB/TPH",    "n",     ["RARE"], "consonant `n` split between strokes"],
  "p/p":    ["-P/P",       "p",     ["RARE"], "consonant `p` split between strokes"],
  "q/qu":   ["-BG/KW",     "qu",    ["RARE"], "`q` slurred into `u` in next stroke"],
  "r/r":    ["-R/R",       "r",     ["RARE"], "consonant `r` split between strokes"],
  "s/s":    ["-S/S",       "s",     ["RARE"], "consonant `s` split between strokes"],
  "s/s+":   ["-Z/S",       "s",     ["RARE"], "consonant `s` split between strokes"],
  "t/t":    ["-T/T",       "t",     ["RARE"], "consonant `t` split between strokes"],
  "v/v":    ["-F/SR",      "v",     ["RARE"], "consonant `v` split between strokes"],
  "x/":     ["-BG/S",      "x",     ["RARE"], "`x` slurred into next stroke"],
  "x/c":    ["-BG/S",      "xc",    ["RARE"], "`x` slurred into `c` in next stroke"],
  "x/x":    ["-BGS/S",     "x",     ["RARE"], "consonant `x` split between strokes"],
  "z/z":    ["-Z/S*",      "z",     ["RARE"], "consonant `z` split between strokes"],
  "zz+":    ["-T/S",       "zz",    ["RARE"], "Italian `zz` split between strokes"]
}
